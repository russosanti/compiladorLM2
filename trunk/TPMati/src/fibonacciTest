globales
fin-globales;

funcion fibonacci (n:entero):entero;
		var fib:entero;
		var nAux1, nAux2:entero;
		var a, b, c: entero;
	comenzar
		si n < 3 entonces
			fib := 1;
		sino
			nAux1 := n-2;
			nAux2 := n-1;
			a := fibonacci(nAux1);
			b := fibonacci(nAux2);
			fib := a + b;
			
		fin-si;	
	fin-func fib;
	
procedimiento PRINCIPAL();
	var nro, i, rta:entero;
comenzar
	nro:=0;
	i:=1;
	mientras nro < 1 hacer
		mostrar 'Ingrese un numero Entero: ';
		leer nro;
		mostrarln ' ';
	fin-mientras;
	mientras i < nro+1 hacer
		rta := fibonacci (i);
		mostrarln i, ':', rta;
		i:=i+1;
	fin-mientras;
fin-proc;	 

